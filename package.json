{
  "name": "babylogs",
  "version": "1.0.0",
  "description": "Simple API to log breastfeeding",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "serve": "nodemon --watch 'src/**/*.ts' --exec  npx ts-node --files src/index.ts",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "jest --watchAll",
    "add-migration": "npx ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "update-database": "npx ts-node ./node_modules/typeorm/cli.js migration:run"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/express": "^4.17.13",
    "@types/node": "^16.10.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.38",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mock-req-res": "^1.1.3",
    "@types/supertest": "^2.0.11",
    "concurrently": "^6.3.0",
    "jest": "^27.2.5",
    "mock-req-res": "^1.2.0",
    "nodemon": "^2.0.13",
    "sinon": "^11.1.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5"
  }
}
